{
	"name": "progress-bar",
	"version": "1.0.0",
	"description": "Simple implementation of progress bar",
	"scripts": {
		"cz": "git add . && git-cz",
		"dev": "webpack-dev-server",
		"build": "NODE_ENV=prod webpack"
	},
	"devDependencies": {
		"@babel/core": "7.4.4",
		"@babel/plugin-proposal-object-rest-spread": "7.4.4",
		"@babel/preset-env": "7.4.4",
		"@commitlint/cli": "7.5.2",
		"@webpack-blocks/assets": "2.0.0",
		"@webpack-blocks/babel": "2.0.0",
		"@webpack-blocks/core": "2.0.0",
		"@webpack-blocks/dev-server": "2.0.0",
		"@webpack-blocks/eslint": "2.0.0",
		"@webpack-blocks/postcss": "2.0.0",
		"@webpack-blocks/sass": "2.0.0",
		"@webpack-blocks/webpack": "2.0.0",
		"autoprefixer": "9.5.1",
		"clean-webpack-plugin": "2.0.1",
		"commitizen": "3.1.1",
		"css-loader": "2.1.1",
		"cssnano": "4.1.10",
		"cz-leremede-changelog": "1.0.1",
		"eslint": "5.16.0",
		"eslint-loader": "2.1.2",
		"file-loader": "3.0.1",
		"html-webpack-plugin": "3.2.0",
		"husky": "2.2.0",
		"jest": "24.7.1",
		"lint-staged": "8.1.6",
		"mini-css-extract-plugin": "0.6.0",
		"prettier": "1.17.0",
		"pretty-quick": "1.10.0",
		"sass-loader": "7.1.0",
		"style-loader": "0.23.1",
		"webpack": "4.30.0",
		"webpack-cli": "3.3.2",
		"webpack-dev-server": "3.3.1"
	},
	"dependencies": {
		"babel-eslint": "10.0.1",
		"d3": "5.9.2",
		"eslint-config-prettier": "4.2.0",
		"eslint-plugin-prettier": "3.0.1"
	},
	"lint-staged": {
		"*.(js|json|html|scss)": [
			"pretty-quick --staged"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "test -e .git/MERGE_HEAD || lint-staged",
			"prepare-commit-msg": "exec < /dev/tty && git cz --hook"
		}
	},
	"_moduleAliases": {
		"@root": ".",
		"@client": "./src"
	},
	"browserslist": [
		"last 2 versions",
		"ie >= 9",
		"safari >= 8"
	],
	"config": {
		"commitizen": {
			"path": "cz-leremede-changelog"
		}
	},
	"author": "Tuan Nguyen <vdtn359@gmail.com>",
	"license": "ISC"
}
